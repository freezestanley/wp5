    "**/*.sass"
  ],
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack serve --hot-only --open 'Google Chrome' --mode development -c ./config/dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --mode production -c ./config/prd.config.js",
    "analy": "cross-env NODE_ENV=analysize webpack --mode production -c ./config/analy.config.js",
    "prettier": "prettier --write './src/**/*.js' './src/**/*.jsx' './src/**/*.tsx' './src/**/*.ts' './src/**/*.css' './src/**/*.scss' './src/**/*.less'",
    "lint-staged": "npm run lint:js && npm run lint:style && npm run prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css,less,json,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "npm run lint-staged:js"
